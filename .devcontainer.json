{
  "name": "macos",
  "service": "macos",
  "forwardPorts": [8006],
  "portsAttributes": {
    "8006": {
      "label": "Web",
      "onAutoForward": "openBrowser"
    },
    "*": {
      "onAutoForward": "silent"
    }    
  },
  "hostRequirements": {
    "storage": "64gb"
  },  
  "dockerComposeFile": ".github/codespaces.yml"
}
