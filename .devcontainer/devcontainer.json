{
  "name": "macOS 14 (Sonoma)",
  "service": "macos",
  "containerEnv": {
    "VERSION": "14"
  },  
  "forwardPorts": [8006],  
  "portsAttributes": {
    "8006": {
      "label": "Web",
      "onAutoForward": "openBrowser"
    }
  },
  "otherPortsAttributes": {
    "onAutoForward": "silent"
  }, 
  "hostRequirements": {
    "storage": "64gb"
  },  
  "dockerComposeFile": "codespaces.yml"
}
